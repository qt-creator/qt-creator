      <context name="PreprocessorEndOfLineSpace" attribute="Preprocessor" lineEndContext="#pop#pop" fallthroughContext="#pop#pop!LineError">
        <IncludeRules context="jinja_boot##Jinja" />
        <DetectSpaces />
        <IncludeRules context="match comments" />
