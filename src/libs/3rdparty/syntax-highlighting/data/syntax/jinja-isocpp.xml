        <RegExpr context="Number" String="\.?\d" lookAhead="true" />
        
        <IncludeRules context="match comments" />
        
        <AnyChar attribute="Separator Symbol" context="#stay" String=",;" />
        <AnyChar attribute="Symbol" context="#stay" String=":(){}[].+-/*%=&lt;&gt;!|&amp;?~^" />
        
