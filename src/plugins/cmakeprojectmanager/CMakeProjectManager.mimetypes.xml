    <sub-class-of type="text/plain"/>
    <comment>CMake Project file</comment>
    <glob pattern="*.cmake"/>
  </mime-type>
  <mime-type type="text/x-cmake-project">
    <sub-class-of type="text/x-cmake"/>
    <comment>CMake Project file</comment>
    <glob pattern="CMakeLists.txt"/>
  </mime-type>
</mime-info>
